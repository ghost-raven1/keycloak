#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/ldap-server-mock@6.0.1/node_modules/ldap-server-mock/node_modules:/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/ldap-server-mock@6.0.1/node_modules:/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/ldap-server-mock@6.0.1/node_modules/ldap-server-mock/node_modules:/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/ldap-server-mock@6.0.1/node_modules:/Users/aleksejstarodubcev/Downloads/keycloak-main/js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ldap-server-mock/server.js" "$@"
else
  exec node  "$basedir/../ldap-server-mock/server.js" "$@"
fi
